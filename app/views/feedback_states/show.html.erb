<% @feedback_state.feedbacks.each do |feedback| %>
  <div>
    <p><%= feedback.description %></p>
    <p>Weighting: <%= feedback.weighting %></p>
    <ul>
      <% feedback.tickets.each do |ticket| %>
        <li><%= link_to ticket.title, ticket.remote_url %></li>
      <% end %>
    </ul>
    <ul>
      <% feedback.customers.each do |customer| %>
        <li><%= link_to customer.name, customer.remote_url %></li>
      <% end %>
    </ul>
    <ul>
      <% feedback.customer_interests.each do |interests| %>
        <li> <%= interests.importance %> </li>
      <% end %>
    </ul>
  </div>
<% end %>
